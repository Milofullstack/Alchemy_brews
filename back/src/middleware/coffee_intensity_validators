function validateCreateIntensity (req, res, next){
    let {name, strength, icon} = req.body;
    if(!name || !strength || !icon){
        res.status(400).json({
            success:false,
            error: "VALIDATION_ERROR",
            msg: " all fields are required"
        });
    }
